        <argsstring>(Vertices subject, Vertices clip, PolyClipType clipType, out PolyClipError error)</argsstring>
        <name>Execute</name>
        <param>
          <type><ref refid="classtainicom_1_1_aether_1_1_physics2_d_1_1_common_1_1_vertices" kindref="compound">Vertices</ref></type>
